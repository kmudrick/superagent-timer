{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-case-declarations": 0,
    "no-param-reassign": [2, { "props": false }],
    "max-len": [2, 200],
    "strict": [2, "safe"],
    "consistent-return": 0,
    "new-cap": 0,
    "global-require": 0,
    "brace-style": 0,
    "arrow-body-style": 0,
    "no-unused-vars": [2, { argsIgnorePattern: 'res|err' }],
    "no-underscore-dangle": 0,
    "import/newline-after-import": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": 1
    },
    "sourceType": "script"
  }
}
